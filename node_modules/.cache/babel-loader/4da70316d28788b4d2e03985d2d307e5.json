DADA;UAEAV;QAFA,CADA;MAFA,CARA;MAiBAW;QAAAC;QAAAC;MAAA;IAjBA;EAmBA,CAvBA;;EAwBAC;IACA;IACAC,MACAC,GADA,CAEAC,0BACA,gCADA,GAEAC,4BAJA,EAKA;MACAC;QACAC;MADA;IADA,CALA,EAWAC,IAXA,CAWAC;MACA;QACAA;UACA;YACA;cACA;cACA;cACA;cACA;;YACA;cACA;cACA;cACA;cACA;;YACA;cACA;cACA;cACA;cACA;UAfA;;UAiBA;QACA,CAnBA;QAoBAC;MACA;IACA,CAnCA;EAoCA;;AA9DA","names":["components","PieChartComponent","name","data","eficiencia","concluido","noConcluido","pendiente","cursosTotal","usuario","chartdata","labels","datasets","backgroundColor","options","responsive","maintainAspectRatio","beforeMount","axios","get","process","sessionStorage","headers","Authorization","then","res","console"],"sourceRoot":"src/components/subpaginas","sources":["PerfilComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-3\">\r\n    <b-card no-body class=\"overflow-hidden\">\r\n      <b-row no-gutters>\r\n        <b-col md=\"2\">\r\n          <b-avatar class=\"m-4\" size=\"5em\">{{\r\n            $parent.usuarioActivo.nombre\r\n          }}</b-avatar>\r\n        </b-col>\r\n        <b-col md=\"10\">\r\n          <b-card-body>\r\n            <b-card-title\r\n              ><b-icon-person-lines-fill></b-icon-person-lines-fill> Datos de la\r\n              cuenta</b-card-title\r\n            >\r\n            <b-card-text>\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6\">\r\n                  <p>\r\n                    <b>Nombre: </b\r\n                    >{{\r\n                      $parent.usuarioActivo.nombre +\r\n                      \" \" +\r\n                      $parent.usuarioActivo.primer_apell +\r\n                      \" \" +\r\n                      $parent.usuarioActivo.segundo_apell\r\n                    }}\r\n                  </p>\r\n                  <p>\r\n                    <b>Usuario/CURP: </b>{{ $parent.usuarioActivo.usuario }}\r\n                  </p>\r\n                  <p>\r\n                    <b>Nivel Académico: </b\r\n                    >{{ $parent.usuarioActivo.preparacion }}\r\n                  </p>\r\n                  <p>\r\n                    <b>Teléfono Móvil: </b\r\n                    >{{ $parent.usuarioActivo.telefono_movil }}\r\n                  </p>\r\n                  <p>\r\n                    <b>Correo electrónico: </b>{{ $parent.usuarioActivo.email }}\r\n                  </p>\r\n                  <p v-if=\"$parent.usuarioActivo.perfil\">\r\n                    <b>Perfil: </b\r\n                    >{{ $parent.usuarioActivo.perfil.descripcion }}\r\n                  </p>\r\n                </div>\r\n                <div class=\"col-md-6\">\r\n                  <strong v-if=\"cursosTotal\">Eficiencia terminal:</strong>\r\n                  <pie-chart-component\r\n                    v-if=\"cursosTotal\"\r\n                    :chartData=\"chartdata\"\r\n                    :options=\"options\"\r\n                  />\r\n                </div>\r\n              </div>\r\n            </b-card-text>\r\n          </b-card-body>\r\n        </b-col>\r\n      </b-row>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport PieChartComponent from \"../graficas/PieChartComponent.vue\";\r\n\r\nexport default {\r\n  components: { PieChartComponent },\r\n  name: \"PerfilComponent\",\r\n  data() {\r\n    return {\r\n      eficiencia: {\r\n        concluido: 0,\r\n        noConcluido: 0,\r\n        pendiente: 0,\r\n      },\r\n      cursosTotal: 0,\r\n      usuario: {},\r\n      chartdata: {\r\n        labels: [\"Concluidos: 0\", \"No Concluidos: 0\", \"Pendientes: 0\"],\r\n        datasets: [\r\n          {\r\n            backgroundColor: [\"#ADD698\", \"#D65D5D\", \"#f2cb52\"],\r\n            data: [0, 0, 0],\r\n          },\r\n        ],\r\n      },\r\n      options: { responsive: true, maintainAspectRatio: false },\r\n    };\r\n  },\r\n  beforeMount() {\r\n    this.usuario = this.$parent.usuarioActivo;\r\n    axios\r\n      .get(\r\n        process.env.VUE_APP_URL +\r\n          \"/registros/eficiencia/usuario/\" +\r\n          sessionStorage.getItem(\"id\"),\r\n        {\r\n          headers: {\r\n            Authorization: \"bearer \" + sessionStorage.getItem(\"token\"),\r\n          },\r\n        }\r\n      )\r\n      .then((res) => {\r\n        if (res.data.length) {\r\n          res.data.forEach((el) => {\r\n            switch (el._id) {\r\n              case \"CONCLUIDO\":\r\n                this.eficiencia.concluido = el.cuenta;\r\n                this.chartdata.datasets[0].data[0] = el.cuenta\r\n                this.chartdata.labels[0] = `Concluidos: ${el.cuenta}`\r\n                break;\r\n              case \"NO CONCLUIDO\":\r\n                this.eficiencia.noConcluido = el.cuenta;\r\n                this.chartdata.datasets[0].data[1] = el.cuenta;\r\n                this.chartdata.labels[1] = `No Concluidos: ${el.cuenta}`\r\n                break;\r\n              case \"PENDIENTE\":\r\n                this.eficiencia.pendiente = el.cuenta;\r\n                this.chartdata.datasets[0].data[2] = el.cuenta;\r\n                this.chartdata.labels[2] = `Pendientes: ${el.cuenta}`\r\n                break;\r\n            }\r\n            this.cursosTotal += el.cuenta;\r\n          });\r\n          console.log(this.eficiencia);\r\n        }\r\n      });\r\n  },\r\n};\r\n</script>\r\n"]}]}